@model Myvshoponline.Project

<title> Upload Logo</title>


<h4>Upload Logo</h4>
@using (Html.BeginForm("UploadLogo", "Projects", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="ProjectID" value="@ViewBag.ProjectID"/>
    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.Label("Logoss", "Logo", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                <input type="file" name="files" id="file" />
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <a id="btnProject" class="btn btn-default" href="~/Projects/Index"> Projects </a>
                <input id="LetMe" type="submit" value="Upload" class="btn btn-default" style="width:50%;" />
                              
            </div>
        </div>
    </div>


    
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
